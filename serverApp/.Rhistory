# Simulación de datos científicos
set.seed(123)
datos <- data.frame(
x = rnorm(100, mean = 50, sd = 10),  # Variable independiente
y = rnorm(100, mean = 20, sd = 5) + 0.5 * rnorm(100, mean = 50, sd = 10)  # Variable dependiente
)
# Ajustar un modelo de regresión lineal
modelo <- lm(y ~ x, data = datos)
# Resumen del modelo (coeficientes, p-valor, R²)
summary(modelo)
# Visualización con ggplot2
ggplot(datos, aes(x = x, y = y)) +
geom_point() +
geom_smooth(method = "lm", se = FALSE, color = "blue") +
labs(title = "Regresión Lineal en Datos Científicos",
x = "Variable Independiente",
y = "Variable Dependiente") +
theme_minimal()
