newton_raphson <- function(f, df, x0, tol = 1e-6, max_iter = 100) {
x <- x0
for (i in 1:max_iter) {
x_new <- x - f(x) / df(x)
if (abs(x_new - x) < tol) {
return(list(raiz = x_new, iteraciones = i))
}
x <- x_new
}
stop("El método no convergió")
}
